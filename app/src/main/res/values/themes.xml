<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Sakabookujicoba" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/putih</item>
        <item name="colorPrimaryVariant">@color/putih</item>
        <item name="colorOnPrimary">@color/putih</item>
        <item name="android:colorFocusedHighlight">@color/hijau</item>
